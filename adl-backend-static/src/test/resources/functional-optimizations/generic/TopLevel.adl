
@Run
composite generic.TopLevel extends ApplicationType {
  
  contains GenericComposite<Client, Server> as subComp1 @Singleton composite {};
  contains GenericComposite<Client, Server> as subComp2 @Singleton composite {};
  contains GenericComposite<Client, GenericComposite<Client, Server>> as subComp3 @Singleton composite {};
  
  binds this.main to subComp1.main;
}